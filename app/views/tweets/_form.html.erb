<%= form_with(model: tweet, local: true) do |form| %>
      <%= form.text_area :text, placeholder: "タイトル" %>
      <%= form.label :prefecture, "撮影場所" %>
      <%= form.collection_select :prefecture, Prefecture.all, :id, :name, {include_blank: "---"}, {required: true}  %>
      <%= form.file_field :image %>
      <%= form.label :rank, "評価" %>
      <%= form.collection_select :rank, Rank.all, :id, :name, {include_blank: "---"}, {required: true}  %>
      <%= form.text_area :text, placeholder: "text", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>